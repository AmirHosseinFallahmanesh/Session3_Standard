
@{
    ViewData["Title"] = "Index";
}

@model  IEnumerable<Demo.Models.Student>


<h1>Student Dashboard</h1>
<a class="btn btn-success" href="~/Student/Create">Create</a>
<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>Surname</th>
        <th>age</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Surname</td>
            <td>@item.Age</td>
            <td>
                <a  class="btn btn-warning" href="~/student/edit/@item.StudentId">Edit</a>
            </td>
        </tr>
    }

</table>

